{
  "expo": {
    "name": "Syno",
    "slug": "syno",
    "version": "1.8.0",
    "description": "Smart medication reminders to help you stay on schedule and healthy.",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f1f8ff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barcelonacodeschool.syno",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 8,
      "package": "com.barcelonacodeschool.syno",
      "permissions": [
        "POST_NOTIFICATIONS",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#fff",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b82e1ff5-fa99-4043-8433-42eeb59dbda0"
      }
    }
  }
}
